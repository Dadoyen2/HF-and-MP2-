prefix=/usr/local
exec_prefix=${prefix}
includedir=${prefix}/include
libdir=${exec_prefix}/lib

Name: trexio
Description: TREX Input/Output library
URL: https://github.com/trex-coe/trexio
Version: 2.5.0
Cflags:  -I${includedir}
Libs: -L${libdir} -ltrexio
Libs.private: -lm -L/usr/lib/x86_64-linux-gnu/hdf5/serial -lhdf5  
Requires: hdf5
Requires.private: hdf5


